<p id="notice"><%= notice %></p>

<h1>Listing Attribute References</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attribute_references.each do |attribute_reference| %>
      <tr>
        <td><%= link_to 'Show', attribute_reference %></td>
        <td><%= link_to 'Edit', edit_attribute_reference_path(attribute_reference) %></td>
        <td><%= link_to 'Destroy', attribute_reference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attribute reference', new_attribute_reference_path %>
